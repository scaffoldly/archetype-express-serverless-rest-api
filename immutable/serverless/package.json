{
  "name": "{{ repository-name }}",
  "version": "1.0.0-0",
  "license": "UNLICENSED",
  "scripts": {
    "prepare": "yarn dotenv && yarn openapi{% if persistence == 'dynamodb' %} && yarn types && yarn dynamodb{% endif %} && yarn tsoa && yarn build",
    "build": "tsc",
    "start": "serverless offline start",
    "deploy": "serverless deploy",
    "serverless": "SLS_DEBUG=* serverless",
    "dotenv": "dotenv-out -f typescript -e .env -e .scaffoldly/.env -c $NODE_ENV -o src",
    "openapi": "openapi-generator -g axios -i .scaffoldly/$NODE_ENV -o src/services/openapi",
    "types": "ts-node types.ts",
    "tsoa": "node tsoa.js",
    "dynamodb": "serverless dynamodb install",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix && yarn run prettier --write '*/**/*.{js,ts,tsx}'"
  },
  "devDependencies": {
    "@scaffoldly/openapi-generator": "^1.0.20",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.17.0",
    "babel-eslint": "^10.1.0",
    "dotenv-out": "^1.0.4",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.19.0",
    "prettier": "^2.3.0",
    "serverless": "^2.2.0",
    "serverless-bundle": "^4.2.3",
    "serverless-configuration": "^1.0.0",
    "serverless-dotenv-plugin": "^3.0.0",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.8.0",
    "serverless-plugin-resource-tagging": "^1.1.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@scaffoldly/serverless-util": "^3.0.62",
    "@types/aws-lambda": "^8.10.77",
    "@vendia/serverless-express": "^4.3.7",
    "axios": "^0.21.1",
    "express": "^4.17.1",
    "joi": "^17.4.0",
    "joi-to-typescript": "^1.12.0",
    "moment": "^2.29.1",
    "tsoa": "^3.8.0",
    "ulid": "^2.3.0"
  }
}
